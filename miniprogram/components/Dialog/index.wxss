.fixed_dialog_wrap {
  width: 100vw;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
}
.fixed_dialog_wrap .dialog_wrap {
  width: 100%;
  height: 100%;
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
}
.fixed_dialog_wrap .dialog_wrap .mask {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  transition: all ease 0.3s;
}
.fixed_dialog_wrap .dialog_wrap .dialog {
  width: 80vw;
  padding-bottom: 0;
  box-sizing: border-box;
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: center;
  transition: all ease 0.3s;
  transform-origin: center center;
  background-color: white;
  color: black;
  border-radius: 24rpx;
  overflow: hidden;
}
.fixed_dialog_wrap .dialog_wrap .dialog.bottom {
  width: 100vw;
  position: absolute;
  bottom: 0;
  border-bottom-left-radius: 0;
  border-bottom-right-radius: 0;
  border-radius: 40rpx;
}
.fixed_dialog_wrap .dialog_wrap .dialog.bottom .options_wrap {
  padding-bottom: calc(36rpx +  env(safe-area-inset-bottom));
}
.fixed_dialog_wrap .dialog_wrap .dialog .icon_close {
  width: 16rpx;
  height: 16rpx;
  border-radius: 32rpx;
  border: 12rpx solid transparent;
  display: inline-block;
  position: absolute;
  top: 30rpx;
  right: 30rpx;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  background-color: rgba(0, 0, 0, 0.2);
  background-image: url('data:image/svg+xml;base64,');
}
.fixed_dialog_wrap .dialog_wrap .dialog .content_wrap {
  width: 100%;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 0 36rpx;
}
.fixed_dialog_wrap .dialog_wrap .dialog .content_wrap .title {
  padding-top: 48rpx;
  font-size: 32rpx;
  line-height: 1;
}
.fixed_dialog_wrap .dialog_wrap .dialog .content_wrap .content {
  width: 100%;
  padding-bottom: 40rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 30rpx;
}
.fixed_dialog_wrap .dialog_wrap .dialog .options_wrap {
  width: 100%;
  box-sizing: border-box;
  padding: 36rpx;
}
.fixed_dialog_wrap .dialog_wrap .dialog .options_wrap .options {
  width: 100%;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
}
.fixed_dialog_wrap .dialog_wrap .dialog .options_wrap .options.cancelUnvisible {
  flex-direction: column-reverse;
  justify-content: start;
  align-items: center;
}
.fixed_dialog_wrap .dialog_wrap .dialog .options_wrap .options.cancelUnvisible .btn.btn_cancel {
  display: none;
  color: #aaa;
  background-color: transparent;
}
.fixed_dialog_wrap .dialog_wrap .dialog .options_wrap .options.cancelUnvisible .btn.btn_cancel:active {
  background-color: transparent;
}
.fixed_dialog_wrap .dialog_wrap .dialog .options_wrap .options.cancelUnvisible .btn.btn_ok {
  width: 100%;
}
.fixed_dialog_wrap .dialog_wrap .dialog .options_wrap .options .btn {
  width: calc(50% - 12rpx);
  height: 88rpx;
  padding: 0;
  margin: 0;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
  font-weight: normal;
  line-height: 1;
  letter-spacing: 2rpx;
  background-color: transparent;
  border-radius: 100rpx;
}
.fixed_dialog_wrap .dialog_wrap .dialog .options_wrap .options .btn::after {
  display: none;
}
.fixed_dialog_wrap .dialog_wrap .dialog .options_wrap .options .btn.btn_cancel {
  color: black;
  background-color: whitesmoke;
}
.fixed_dialog_wrap .dialog_wrap .dialog .options_wrap .options .btn.btn_cancel:active {
  background-color: #ddd;
}
.fixed_dialog_wrap .dialog_wrap .dialog .options_wrap .options .btn.btn_ok {
  background-color: #222;
  color: white;
}
.fixed_dialog_wrap .dialog_wrap .dialog .options_wrap .options .btn.btn_ok:active {
  filter: brightness(1.2);
}
