@keyframes light {
  from {
    clip: rect(60rpx 60rpx 60rpx 0);
  }
  to {
    clip: rect(0 60rpx 60rpx 0);
  }
}
.init {
  font-size: 15px;
  line-height: 1.6;
  color: black;
}
.loading_wrap {
  font-size: 15px;
  line-height: 1.6;
  color: black;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1000;
  width: 100vw;
  height: 100vh;
  background-color: transparent;
  display: flex;
  justify-content: center;
  align-items: center;
}
.loading_wrap.maskVisible {
  background-color: white;
}
.loading_wrap .loading {
  position: relative;
  width: 180rpx;
  height: 180rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background-color: rgba(0, 0, 0, 0.9);
  border-radius: 24rpx;
}
.loading_wrap .loading .icon {
  width: 60rpx;
  height: 60rpx;
}
.loading_wrap .loading .icon_source {
  width: 60rpx;
  height: 60rpx;
  background-size: 100% 100%;
  transform-origin: center bottom;
  background-image: url('data:image/svg+xml;base64,');
}
.loading_wrap .loading .icon_source_bottom {
  position: absolute;
  opacity: 0.3;
}
.loading_wrap .loading .icon_source_top {
  position: absolute;
  animation: light 1s infinite;
}
