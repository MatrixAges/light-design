<view class="example_items mini_items column_2 visible_items w_100 border_box flex flex_column">
      <view class="example_item w_100 border_box flex">
            <view class="title_wrap border_box flex flex_column">
                  <text class="title">默认效果</text>
                  <text class="attr">default</text>
            </view>
            <light-icon
                  icon="{{blockRight}}"
                  color="white"
                  fillColor="black"
                  visibleWrap
                  filled
                  round
                  mark:type="default"
                  catchtap="onOption"
            ></light-icon>
            <light-landing visible="{{visible_default}}"></light-landing>
      </view>
      <view class="example_item w_100 border_box flex">
            <view class="title_wrap w_100 border_box flex flex_column">
                  <text class="title">显示遮罩</text>
                  <text class="attr">maskVisible</text>
            </view>
            <light-icon
                  icon="{{blockRight}}"
                  color="white"
                  fillColor="black"
                  visibleWrap
                  filled
                  round
                  mark:type="maskVisible"
                  catchtap="onOption"
            ></light-icon>
            <light-landing
                  visible="{{visible_maskVisible}}"
                  maskVisible
            ></light-landing>
      </view>
      <view class="example_item w_100 border_box flex">
            <view class="title_wrap w_100 border_box flex flex_column">
                  <text class="title">遮罩颜色</text>
                  <text class="attr">maskColor</text>
            </view>
            <light-icon
                  icon="{{blockRight}}"
                  color="white"
                  fillColor="black"
                  visibleWrap
                  filled
                  round
                  mark:type="maskColor"
                  catchtap="onOption"
            ></light-icon>
            <light-landing
                  visible="{{visible_maskColor}}"
                  maskColor="rgba(255, 255, 255, 0.8)"
                  maskVisible
            ></light-landing>
      </view>
</view>