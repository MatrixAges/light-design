<view class="example_items w_100 border_box flex flex_column">
      <view class="example_item w_100 border_box flex">
            <view class="title_wrap w_100 border_box flex flex_column">
                  <text class="title">默认效果</text>
                  <text class="attr">center</text>
            </view>
            <view class="examples flex flex_wrap">
                  <view class="example">
                        <_Button
                              mark:type="default"
                              catchtap="onOption"
                        >Show</_Button>
                        <_Modal visible="{{visible_default}}">
                              <view class="bg_white p_32 radius_10 border_box">居中显示的内容</view>
                        </_Modal>
                  </view>
            </view>
      </view>
      <view class="example_item w_100 border_box flex">
            <view class="title_wrap w_100 border_box flex flex_column">
                  <text class="title">顶部弹出</text>
                  <text class="attr">top</text>
            </view>
            <view class="examples flex flex_wrap">
                  <view class="example">
                        <_Button
                              mark:type="top"
                              catchtap="onOption"
                        >Show</_Button>
                        <_Modal
                              visible="{{visible_top}}"
                              position="top"
                        >
                              <view class="bg_white p_32 radius_10 border_box">居中显示的内容</view>
                        </_Modal>
                  </view>
            </view>
      </view>
      <view class="example_item w_100 border_box flex">
            <view class="title_wrap w_100 border_box flex flex_column">
                  <text class="title">底部弹出</text>
                  <text class="attr">bottom</text>
            </view>
