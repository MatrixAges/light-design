<view class="example_items mini_items column_2 visible_items w_100 border_box flex flex_column">
      <view class="example_item w_100 border_box flex">
            <view class="title_wrap w_100 border_box flex flex_column">
                  <text class="title">默认效果</text>
                  <text class="attr">center</text>
            </view>
            <_Icon
                  icon="blockRight"
                  color="white"
                  fillColor="black"
                  visibleWrap
                  filled
                  round
                  mark:type="default"
                  catchtap="onOption"
            ></_Icon>
            <_Modal visible="{{visible_default}}">
                  <view class="bg_white p_32 radius_10 border_box">居中显示的内容</view>
            </_Modal>
      </view>
