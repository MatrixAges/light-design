                  <view class="example">
                        <_Button
                              mark:type="left"
                              catchtap="onOption"
                        >Show</_Button>
                        <_Modal
                              visible="{{visible_left}}"
                              position="left"
                        >
                              <view class="bg_white p_32 radius_10 border_box">居中显示的内容</view>
                        </_Modal>
                  </view>
            </view>
      </view>
      <view class="example_item w_100 border_box flex">
            <view class="title_wrap w_100 border_box flex flex_column">
                  <text class="title">右侧弹出</text>
                  <text class="attr">right</text>
            </view>
            <view class="examples flex flex_wrap">
                  <view class="example">
                        <_Button
                              mark:type="right"
                              catchtap="onOption"
                        >Show</_Button>
                        <_Modal
                              visible="{{visible_right}}"
