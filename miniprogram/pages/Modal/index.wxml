                  <view class="example">
                        <_Button
                              mark:type="bottom"
                              catchtap="onOption"
                        >Show</_Button>
                        <_Modal
                              visible="{{visible_bottom}}"
                              position="bottom"
                        >
                              <view class="bg_white p_32 radius_10 border_box">居中显示的内容</view>
                        </_Modal>
                  </view>
            </view>
      </view>
      <view class="example_item w_100 border_box flex">
            <view class="title_wrap w_100 border_box flex flex_column">
                  <text class="title">左侧弹出</text>
                  <text class="attr">left</text>
            </view>
